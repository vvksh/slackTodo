generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        Int      @id @default(autoincrement())
  userId    String   @map("user_id")
  task      String
  completed Boolean  @default(false)
  createdAt DateTime @default(now()) @map("created_at")

  @@map("todos")
}